{
  "name": "xcedu-web-common",
  "version": "1.0.0",
  "description": "晓窗科技教育一体化平台公共模块",
  "main": "dist/vendor/vendor.min.js",
  "repository": "git@github.com:xuanhui919/xcedu-web-common.git",
  "author": "Xuan Hui(xuanhui919) <541951523@qq.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./themes ./temp ./build ./dist",
    "theme": "npm-run-all theme:*",
    "gulp": "gulp build",
    "serve": "run-s clean theme && gulp build --env development && serve -p 8490",
    "serve:prod": "npm run build:deploy && serve -p 8990 &",
    "theme:green": "et -c src/themes/variables-green.scss -o themes/green",
    "theme:blue": "et -c src/themes/variables-blue.scss -o themes/blue",
    "theme:orange": "et -c src/themes/variables-orange.scss -o themes/orange",
    "theme:red": "et -c src/themes/variables-red.scss -o themes/red",
    "build:deploy": "run-s clean theme gulp",
    "lint": "npm-run-all lint:*",
    "lint:scripts": "eslint --ext js,vue,ts gulpfile.js",
    "lint:styles": "stylelint ./src/**/*.{vue,scss,sass,css,less}",
    "format": "npm-run-all format:*",
    "format:scripts": "eslint --fix --ext js,vue,ts gulpfile.js",
    "format:styles": "stylelint --fix ./src/**/*.{vue,scss,sass,css,less}"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "element-theme": "^2.0.1",
    "element-theme-chalk": "^2.13.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.2.0",
    "gulp-cli": "^2.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-exports": "^0.0.3",
    "gulp-filter": "^6.0.0",
    "gulp-gzip": "^1.4.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "gulp-umd": "^2.0.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "serve": "^11.3.0",
    "stylelint": "10",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "yargs": "^15.3.1"
  },
  "dependencies": {
    "axios": "0.19.2",
    "element-ui": "2.13.0",
    "import-map-overrides": "^1.14.3",
    "normalize.css": "^8.0.1",
    "nprogress": "0.2.0",
    "single-spa": "^5.3.1",
    "single-spa-vue": "^1.8.2",
    "systemjs": "^6.3.0",
    "systemjs-webpack-interop": "^2.0.0",
    "vue": "2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "8.4.0",
    "vue-router": "3.1.6",
    "vuex": "3.1.2",
    "vuex-class": "^0.3.2",
    "vuex-router-sync": "^5.0.0"
  }
}
